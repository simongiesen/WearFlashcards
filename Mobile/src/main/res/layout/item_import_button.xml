<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentEnd="true"
    tools:context=".activities.ImportSetsActivity">

    <Button
        android:id="@+id/button_import_sets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_marginBottom="@dimen/activity_vertical_padding"
        android:layout_marginEnd="@dimen/activity_horizontal_padding"
        android:layout_marginTop="@dimen/activity_vertical_padding"
        android:onClick="importSets"
        android:text="@string/button_import"
        android:textColor="@color/text_default" />
</FrameLayout>